// a subset of https://github.com/stuartcarnie/clang/blob/master/include/clang/Basic/TokenKinds.def

keyword(TOKEN_KWINT, "int")
keyword(TOKEN_KWVOID, "void")
keyword(TOKEN_KWFLOAT, "float")
keyword(TOKEN_KWCHAR, "char")
keyword(TOKEN_KWIF, "if")
keyword(TOKEN_KWELSE, "else")
keyword(TOKEN_KWWHILE, "while")
keyword(TOKEN_KWRETURN, "return")

punctuator(TOKEN_PLUS, "+")
punctuator(TOKEN_PLUSPLUS, "++")
punctuator(TOKEN_PLUSEQ, "+=")
punctuator(TOKEN_MINUS, "-")
punctuator(TOKEN_MINUSMINUS, "--")
punctuator(TOKEN_MINUSEQ, "-=")
punctuator(TOKEN_TILDE, "~")
punctuator(TOKEN_EXCLAIM, "!")
punctuator(TOKEN_EXCLAIMEQ, "!=")
punctuator(TOKEN_AMP, "&")
punctuator(TOKEN_AMPAMP, "&&")
punctuator(TOKEN_AMPEQ, "&=")
punctuator(TOKEN_EQ, "=")
punctuator(TOKEN_EQEQ, "==")
punctuator(TOKEN_GREATER, ">")
punctuator(TOKEN_GREATERGREATER, ">>")
punctuator(TOKEN_GREATEREQ, ">=")
punctuator(TOKEN_GREATERGREATEREQ, ">>=")
punctuator(TOKEN_LESS, "<")
punctuator(TOKEN_LESSLESS, "<<")
punctuator(TOKEN_LESSEQ, "<=")
punctuator(TOKEN_LESSLESSEQ, "<<=")
punctuator(TOKEN_CARET, "^")
punctuator(TOKEN_CARETEQ, "^=")
punctuator(TOKEN_PIPE, "|")
punctuator(TOKEN_PIPEPIPE, "||")
punctuator(TOKEN_PIPEEQ, "|=")
punctuator(TOKEN_STAR, "*")
punctuator(TOKEN_STAREQ, "*=")
punctuator(TOKEN_SLASH, "/")
punctuator(TOKEN_SLASHEQ, "/=")
punctuator(TOKEN_LSQUARE, "[")
punctuator(TOKEN_RSQUARE, "]")
punctuator(TOKEN_LPAREN, "(")
punctuator(TOKEN_RPAREN, ")")
punctuator(TOKEN_LBRACE, "{")
punctuator(TOKEN_RBRACE, "}")
punctuator(TOKEN_SEMI, ";")
punctuator(TOKEN_PERIOD, ".")
punctuator(TOKEN_COMMA, ",")
